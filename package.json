{
  "name": "ai-development-pipeline",
  "version": "1.0.0",
  "description": "Claude/VSCode/Augment AI pipeline with unified MCP, Vercel, Airtable, Square integration",
  "main": "local-mcp-server.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:mcp": "npx ts-node local-mcp-server.ts",
    "start:local": "npx ts-node local-mcp-server.ts",
    "cloud-mcp": "vercel dev",
    "deploy": "vercel deploy --prod",
    "install:global": "npm install -g ts-node nodemon",
    "lint": "npx eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "echo 'No tests yet.'",
    "type-check": "npx tsc --noEmit",
    "clean": "rm -rf dist .next"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.58",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "Claude",
    "AI",
